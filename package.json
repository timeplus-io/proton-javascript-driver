{
  "name": "@timeplus/proton-javascript-driver",
  "version": "0.1.1",
  "description": "Streaming REST client for Timeplus Proton",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timeplus-io/proton-javascript-driver.git"
  },
